{
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": true,
    "paths": {
      "@actions/*": [
        "./actions/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@context/*": [
        "./context/*"
      ],
      "@reducers/*": [
        "./reducers/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@public/*": [
        "./public/*"
      ],
      "@pages/*": [
        "app/pages/*"
      ],
      "@typings/*": [
        "./typings/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next.config.js",
    ".next/types/**/*.ts"
  ]
}
